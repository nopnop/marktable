{
  "name": "marktable",
  "version": "0.0.1",
  "description": "Markdown table parser, editor and formater ",
  "main": "lib/marktable.js",
  "scripts": {
    "test": "mocha",
    "spec": "mocha --reporter spec",
    "monitor": "mocha --reporter spec --watch",
    "debugtest": "mocha --debug-brk --reporter spec",
    "coverage": "rm -rf ./lib-cov && jscoverage ./lib ./lib-cov && COVERAGE=true mocha --reporter html-cov > ./test/coverage.html && open ./test/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nopnop/marktable.git"
  },
  "keywords": [
    "markdown",
    "gfm",
    "table",
    "parser",
    "formater",
    "html",
    "multiline"
  ],
  "author": "Jean Ponchon <jean.ponchon@novadiscovery.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nopnop/marktable/issues"
  },
  "homepage": "https://github.com/nopnop/marktable",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "devDependencies": {
    "chokidar": "^1.0.3",
    "cli-color": "^1.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "underscore": "^1.6.0",
    "underscore.string": "^2.3.3"
  }
}
